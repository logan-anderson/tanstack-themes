{
  "name": "tanstack-themes",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logan-anderson/tanstack-themes"
  },
  "exports": {
